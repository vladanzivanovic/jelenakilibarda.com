{
    "type": "project",
    "name": "vladapoz/kilibarda-biography",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "description": "Biography website",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "friendsofsymfony/jsrouting-bundle": "^2.8",
        "gedmo/doctrine-extensions": "^3.5",
        "liip/imagine-bundle": "^2.7",
        "mobiledetect/mobiledetectlib": "^2.8",
        "nelmio/security-bundle": "^2.10",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "presta/sitemap-bundle": "^3.2",
        "ramsey/uuid-doctrine": "^1.8",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/google-mailer": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/messenger": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "5.4.*",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/webapp-meta": "^1.0",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "willdurand/js-translation-bundle": "^5.0"
    },
    "require-dev": {
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "develop": [
            "composer dump-autoload --classmap-authoritative --ansi",
            "composer install",
            "composer dump-autoload --classmap-authoritative --ansi",
            "bin/console doctrine:cache:clear-metadata",
            "bin/console doctrine:migration:migrate --no-interaction",
            "bin/console assets:install --symlink public/js",
            "bin/console fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json",
            "bin/console bazinga:js-translation:dump public/js --format json --merge-domains",
            "npm install",
            "npm audit fix",
            "npm run dev"
        ],
        "production": [
            "composer install --no-dev --optimize-autoloader",
            "composer dump-autoload --optimize --no-dev --classmap-authoritative",
            "composer dump-env prod",
            "bin/console doctrine:migration:migrate --no-interaction -e prod",
            "bin/console cache:clear --no-warmup -e prod --ansi",
            "bin/console cache:warmup -e prod",
            "bin/console assets:install --symlink public/js",
            "npm install",
            "npm audit fix",
            "npm run build",
            "php bin/console presta:sitemaps:dump --gzip",
            "chown -R www-data:www-data ."
        ],
        "setup-app-prod": [
            "composer install --no-dev --optimize-autoloader",
            "composer dump-autoload --optimize --no-dev --classmap-authoritative",
            "composer dump-env prod",
            "bin/console doctrine:migrations:execute \"DoctrineMigrations\\Version20211208093543\" --no-interaction -e prod",
            "bin/console doctrine:migration:migrate --no-interaction -e prod",
            "bin/console cache:clear --no-warmup -e prod --ansi",
            "bin/console cache:warmup -e prod",
            "bin/console assets:install --symlink public/js",
            "npm install",
            "npm audit fix",
            "npm run build",
            "chown -R www-data:www-data ."
        ],
        "cache:clear": [
            "php bin/console cache:clear --no-warmup --ansi",
            "php bin/console cache:warmup"
        ],
        "cache:clear:prod": [
            "php bin/console cache:clear --no-warmup -e prod --ansi",
            "php bin/console cache:warmup -e prod"
        ],
        "route-locale-generate": [
            "php bin/console fos:js-routing:dump --format=json --target=public/js/fos_js_routes.json",
            "php bin/console bazinga:js-translation:dump public/js --format json --merge-domains"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
